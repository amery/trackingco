{
  "name": "trackingco.de-client",
  "dependencies": {
    "auth0-js": "^8.3.0",
    "body-style": "^0.3.0",
    "color": "^1.0.3",
    "leven": "^2.1.0",
    "localsync": "^1.4.0",
    "lokka": "^1.7.0",
    "lokka-transport-http": "^1.6.1",
    "months": "^1.0.1",
    "page": "^1.7.1",
    "porreta": "^1.7.0",
    "randomcolor": "^0.4.4",
    "react": "^15.4.2",
    "react-click-outside": "^2.2.0",
    "react-color": "^2.11.3",
    "react-dnd": "^2.2.3",
    "react-dnd-html5-backend": "^2.2.3",
    "react-dnd-touch-backend": "^0.3.5",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.4.2",
    "react-hyperscript": "^3.0.0",
    "react-tangle": "github:fiatjaf/react-tangle",
    "recharts": "^0.20.8",
    "throttleit": "^1.0.0",
    "tiny-emitter": "^1.1.0",
    "url": "^0.11.0",
    "xtend": "^4.0.1"
  },
  "scripts": {
    "watch": "find -name \"*.js\" ! -name \"bundle.js\" ! -path \"*node_modules*\" | entr browserifyinc -vd app.js -o bundle.js",
    "build": "browserify app.js -o bundle.js",
    "build-prod": "npm run build"
  },
  "browserify": {
    "transform": [
      [
        "bubleify",
        {
          "transforms": {
            "dangerousTaggedTemplateString": true
          },
          "objectAssign": "xtend",
          "sourceMap": true
        }
      ]
    ]
  },
  "devDependencies": {
    "browserify": "^14.1.0",
    "browserify-incremental": "^3.1.1",
    "bubleify": "^0.7.0"
  }
}
